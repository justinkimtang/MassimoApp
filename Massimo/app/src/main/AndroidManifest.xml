<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.landonbedell.massimo">

    <application
        android:name=".OrderController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.landonbedell.massimo.RateActivity" ></activity>
        <activity android:name="com.example.landonbedell.massimo.OrderView"
                    android:label ="Order"></activity>
        <activity android:name="com.example.landonbedell.massimo.PayView"
            android:label ="Pay"></activity>
        <activity
            android:name="com.example.landonbedell.massimo.CameraView">
        </activity>
        <meta-data android:name="com.google.android.gms.vision.DEPENDENCIES" android:value="barcode"/>
        <uses-permission android:name="android.permission.CAMERA" /><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />


    </application>
    <uses-feature android:name="android.hardware.camera" android:required="true" />

</manifest>

